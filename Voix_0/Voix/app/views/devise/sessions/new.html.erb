

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
  
  <div class="log_in_form">
  <h2 class="log_in_text">Log in</h2>
  
    <%= f.email_field :email, autofocus: true, class: "form-control log_email",placeholder:"Email" %>
    <%= f.password_field :password, autocomplete: "off", class: "form-control log_password", placeholder:"Password" %>
    <%= f.submit "Log in", class: "form-control log_submit_button" %>
    <%= render "devise/shared/links" %>

  </div>
<% end %>


